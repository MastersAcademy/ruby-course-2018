<p id="notice"><%= notice %></p>
<h1>Update category</h1>
<%= form_for @category do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>
  <%= form.submit 'Update!' %>
<% end %>