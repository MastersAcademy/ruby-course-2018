<p id="notice"><%= notice %></p>
<%= form_tag users_path do %>
  <div class="field">
    <%= label :user, :email %>
    <%= text_field :user, :email %>
  </div>

  <div class="field">
    <%= label :user, :password %>
    <%= password_field :user, :password %>
  </div>

  <div class="field">
    <%= label :user, :password_confirmation %>
    <%= password_field :user, :password_confirmation %>
  </div>

  <div class="actions">
    <%= submit_tag 'SignUp!' %>
  </div>
<% end %>
